{
    "name": "Activity Tracker v1.0",
    "version": "1.0",
    "description": "Tracks time spent on web",
    "permissions": ["storage", "tabs","debugger","<all_urls>"],
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_title": "AcTker",
        "default_popup": "main.html"
    },
    "web_accessible_resources": [
        "js/*"
    ],
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.gstatic.com https://d3js.org/d3.v3.min.js https://platform.twitter.com; object-src 'self'"
}